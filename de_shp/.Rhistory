data <- read.csv("~/Descargas/Curso_R_FESC_2025_2/datasetWordFrequencyFinal.csv")
dimData <-dim(data)
dimData
averages <- colMeans(data)
averages
class(averages)
is.vector(averages)
length(averages)
#etiquetas <- c("life", "thing", "beer", "kitten", "disgrace", "mocha", "nihilism", "bloke", "gnome", "puffball") #Esta lista no es necesaria, pues ya los nombres de fila vienen dados desde el paso 9.
promedios <- c(averages[3:12]) #No nos interesa el promedio del ID de participante, ni el de sus edades. Por eso empezamos desde la columna 3, donde empiezan los datos de tiempos de lectura.
promedios #Se revisan los promedios para ver si coinciden aproximadamente con los mostrados en la fig. 1.
wordFrequency <- c(4.7, 4.8, 3.7, 2.4, 2.7, 1.9, 0.7, 2.4, 1.5, 0.6 ) #Información aproximada tomada también de la fig. 1.
dfModelo <- data.frame(promedios, wordFrequency)
dfModelo
cor_ResponseTime_wordFrequency <- cor(wordFrequency, promedios)
cor_ResponseTime_wordFrequency
modelo_wordFrequency_promTiemposRespuesta <- lm(promedios ~ wordFrequency, data = dfModelo)
modelo_wordFrequency_promTiemposRespuesta
ggplot(dfModelo, aes(x = wordFrequency, y = promedios)) +
geom_point(color = "blue") +
geom_smooth(method = "lm", se = TRUE, color = "red") +
labs(title = "Regresión Lineal: Frecuencia vs Tiempo de respuesta",
x = "Frecuencia de palabra", y = "Tiempo de respuesta") +
theme_minimal()
summary(modelo_wordFrequency_promTiemposRespuesta)
